{
  "name": "code-smell-gpt",
  "displayName": "CodeSmellGPT",
  "description": "Super Simple AI-based code review extension for programming languages. Automatically review the code you write, give you suggestion, and fix errors with one click.",
  "version": "0.7.5",
  "icon": "images/code-smell-gpt-logo.png",
  "publisher": "clean99",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Linters",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "code-smell-gpt.smellCheckByAll",
        "title": "run code smell"
      }
    ],
    "configuration": {
      "title": "VSCode Code Smell",
      "properties": {
        "code-smell-gpt.gptKey": {
          "type": "string",
          "description": "Config GPT API key"
        }
      }
    }
  },
  "main": "./out/extension.js",
  "homepage": "https://github.com/clean99/vscode-code-smell-gpt",
  "author": {
    "name": "clean99",
    "email": "xff9924@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clean99/vscode-code-smell-gpt.git"
  },
  "bugs": {
    "url": "https://github.com/clean99/vscode-code-smell-gpt/issues"
  },
  "license": "MIT",
  "keywords": [
    "Code Review",
    "Refactor",
    "GPT",
    "OPEN AI",
    "Spell Check",
    "SpellCheck",
    "Code Smells",
    "Code Smell",
    "VS Code",
    "vscode",
    "smell",
    "extension"
  ],
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch"
  },
  "devDependencies": {
    "@types/diff": "^5.0.5",
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.199",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.17.5",
    "@types/vscode": "^1.58.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "esbuild": "^0.19.4",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^5.2.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "@types/minimatch": "^3.0.5",
    "diff": "^5.1.0",
    "lodash": "^4.17.21",
    "openai": "^4.10.0"
  },
  "pricing": "Free"
}
